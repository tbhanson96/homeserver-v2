{
  "$schema": "./node_modules/ng-openapi-gen/ng-openapi-gen-schema.json",
  "input": "../swagger.json",
  "output": "src/app/api",
  "prefix": "Api",
  "ignoreUnusedModels": false,
  "minParamsForContainer": 4,
  "sortParams": "desc",
  "defaultTag": "Api",
  "removeStaleFiles": true,
  "modelIndex": true,
  "serviceIndex": true,
  "module": false,
  "enumStyle": "alias"
}