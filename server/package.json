{
  "name": "homeserver-server",
  "version": "0.0.1",
  "description": "",
  "author": "Tim Hanson",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && npm run swagger",
    "package": "nexe -t linux-x64-12.9.1 -i dist/main.js -o dist/server",
    "swagger": "node swagger-doc-gen.js",
    "start": "node dist/main.js",
    "start:dev": "npm run build && node dev-server.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "6.11.7",
    "@nestjs/core": "6.11.7",
    "@nestjs/jwt": "6.1.2",
    "@nestjs/microservices": "6.11.7",
    "@nestjs/passport": "6.1.0",
    "@nestjs/platform-express": "6.11.7",
    "@nestjs/swagger": "3.1.0",
    "@nestjs/websockets": "6.11.7",
    "@types/http-proxy": "1.17.5",
    "@types/http-proxy-middleware": "0.19.3",
    "@types/nodemailer": "6.4.0",
    "@types/tar": "4.0.4",
    "dotenv": "8.2.0",
    "epub": "1.2.1",
    "http-proxy-middleware": "0.20.0",
    "memfs": "3.1.2",
    "morgan": "1.9.1",
    "ncp": "2.0.0",
    "node-calibre": "2.1.1",
    "nodemailer": "6.4.16",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.3",
    "swagger-ui-express": "4.1.2",
    "tar": "6.1.2"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.2",
    "@nestjs/testing": "6.11.7",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.11",
    "@types/jest": "24.9.1",
    "@types/morgan": "1.7.37",
    "@types/multer": "1.4.2",
    "@types/node": "11.15.52",
    "@types/passport-jwt": "3.0.2",
    "@types/passport-local": "1.0.33",
    "@types/supertest": "2.0.11",
    "jest": "26.0.0",
    "mkdirp": "0.5.1",
    "nexe": "3.3.7",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "supertest": "4.0.2",
    "ts-jest": "24.1.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.10.2",
    "tsc-watch": "4.0.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "typescript": "3.7.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
